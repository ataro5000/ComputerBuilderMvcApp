{
    "version": "0.2.0",
    "configurations": [


        {
            "name": "C#: Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "$C{workspaceFolder}/ComputerBuilderMvcApp.csproj"
        },
        {
            "name": "C#: ComputerBuilderMvcApp", 
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
           
            "program": "${workspaceFolder}/bin/Debug/net9.0/ComputerBuilderMvcApp.dll", 
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
        
                "ASPNETCORE_ENVIRONMENT": "Development" 
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}